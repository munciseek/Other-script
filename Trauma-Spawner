game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()

while true do
wait(120)
coroutine.wrap(function()
        while true do
        end
        end)        
        local random = math.random(1,100)
        print("实体：Trauma 随机数：",random)
        if random >= 0 and random <= 100 then
        
        local spawner = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Entity%20Spawner/V2/Source.lua"))()
        
        ---====== Create entity ======---
        
        local entity = spawner.Create({
            Entity = {
                Name = "Trauma",
                Asset = "https://raw.githubusercontent.com/munciseek/DOORS-entity-models/main/Nightmare/Trauma.rbxm",
                HeightOffset = 0
            },
            Lights = {
                Flicker = {
                    Enabled = true,
                    Duration = 1
                },
                Shatter = true,
                Repair = false
            },
            CameraShake = {
                Enabled = true,
                Range = 100,
                Values = {1.5, 20, 0.1, 1} -- Magnitude, Roughness, FadeIn, FadeOut
            },
            Movement = {
                Speed = 1200,
                Delay = 2,
                Reversed = false
            },
            Rebounding = {
                Enabled = true,
                Type = "Ambush", -- "Blitz"
                Min = 1,
                Max = 1,
                Delay = 0.3
            },
            Damage = {
                Enabled = true,
                Range = 100,
                Amount = 0
            },
            Crucifixion = {
                Enabled = true,
                Range = 100,
                Resist = false,
                Break = true
                },
            Death = {
		        Type = "Guiding", -- "Curious"
                Hints = {"You died to Trauma", "Its sound is very small", "Keep an eye out for flashing lights"},
		        Cause = ""
	}
})
        ---====== Debug entity ======---
        entity:SetCallback("OnDamagePlayer", function(newHealth)
            if newHealth == 0 then
        local achievementGiver = loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Utilities/main/Doors/Custom%20Achievements/Source.lua"))()
        
        achievementGiver({
            Title = "Traumatic eye",
            Desc = "Is it worth it?",
            Reason = "Die to Trauma",
            Image = "rbxassetid://11278624160"
        })
            end
        end)
        
        ---====== Run entity ======---
        
        entity:Run()
        end
        end
